{
  "name": "docs",
  "description": "pota docs",
  "version": "0.1.0",
  "author": "Tito Bouzout",
  "type": "module",
  "homepage": "https://pota.quack.uy/",
  "imports": {
    "#main": "./src/@main.jsx"
  },
  "scripts": {
    "dev": "concurrently --kill-others \"npm:dev:*\"",
    "dev:vite": "vite --force",
    "potadev": "concurrently --kill-others \"npm:potadev:*\"",
    "potadev:vite": "vite --force",
    "potadev:source": "cpx \"../pota/**\" ./node_modules/pota/ -w",
    "potadev:standalone-1": "cpx \"../pota/dist/**\" ./src/public/ -w",
    "potadev:standalone-2": "cpx \"../pota/dist/**\" ./dist/public/ -w",
    "build": "vite build --force",
    "preview": "vite preview"
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "cpx2": "^6.0.1",
    "dom-flair": "^6.0.44",
    "fflate": "^0.8.1",
    "monaco-editor": "^0.44.0",
    "pota": "^0.2.x",
    "vite": "^4.4.6",
    "vite-plugin-babel": "^1.1.3"
  },
  "prettier": {
    "printWidth": 70,
    "useTabs": true,
    "semi": false,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "avoid",
    "proseWrap": "never",
    "endOfLine": "lf",
    "singleAttributePerLine": true
  }
}
